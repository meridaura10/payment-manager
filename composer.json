{
    "name": "meridaura/payment-manager",
    "description": "Пакет для зручної інтеграції та управління платіжними шлюзами",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0|^13.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0|^13.0",
        "illuminate/database": "^10.0|^11.0|^12.0|^13.0"
    },
    "autoload": {
        "psr-4": {
            "Meridaura\\PaymentManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meridaura\\PaymentManager\\PaymentServiceProvider"
            ],
            "aliases": {
                "Payment": "Meridaura\\PaymentManager\\Facades\\PaymentManager"
            }
        }
    },
    "authors": [
        {
            "name": "meridaura"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^10.9"
    }
}
